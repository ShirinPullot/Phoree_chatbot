{
  "version": 2,
  "builds": [
    {
      "src": "src/app/api/chat/route.py",
      "use": "@vercel/python"
    },
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/api/chat/(.*)",
      "dest": "src/app/api/chat/route.py"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}